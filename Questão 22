package questoes_andre;

public class Vinte_Dois {

	public static void main(String[] args) {

		int[][] matriz = new int[3][3];
		int numeroPar = 0;

		for (int i = 0; i < 3; i++) {
			for (int j = 0; j < 3; j++) {

				matriz[i][j] = numeroPar;
				numeroPar += 2;
			}

		}

		for (int i = 0; i < 3; i++) {
			for (int j = 0; j < 3; j++) {
				System.out.println(matriz[i][j] + "\t");

			} System.out.println();

		}

	}
}
